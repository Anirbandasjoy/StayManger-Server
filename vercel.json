{
  "version": 2,
  "builds": [
    {
      "src": "./index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["tsconfig.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
